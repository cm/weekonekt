  "repository": {
    "type": "git",
    "url": "https://git.heroku.com/weekonekt.git"
  },
  "scripts": {
    "web": "webpack-dev-server --hot --inline --content-base src/",
    "build": "rimraf dist && webpack",
    "reinstall": "rimraf node_modules && npm uninstall -g elm && npm i -g elm && npm i && elm package install",
    "push": "npm run build && git add . && git commit -am 'latest changes' && git push heroku master"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "bootstrap-sass": "*",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "*",
    "elm": "^0.18.0",
    "elm-hot-loader": "^0.5.2",
    "elm-webpack-loader": "^3.0.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.28.0",
    "jquery": "^3.1.1",
    "less": "^2.7.1",
    "node-sass": "*",
    "postcss-loader": "^1.1.1",
    "rimraf": "^2.5.2",
    "sass-loader": "*",
    "style-loader": "*",
    "url-loader": "*",
    "webpack": "^2.3.2",
    "webpack-dev-server": "2",
    "webpack-merge": "*"
  }
